{{> NavBar}}

<style>
    #nav_remove {
        border-bottom: solid 2px #f5f2f2;
    }
</style>

<fieldset>

    <legend>
        remove a lobby
    </legend>

    {{#Dto.Lobbies}}

        <form method="post" action="">
            <input class="remove_lobby" type="submit" value="{{LobbyName}}"/>
            <input name="lobby" type="hidden" value="{{LobbyId}}"/>
            <input name="lobby_name" type="hidden" value="{{LobbyName}}"/>
            <input name="best_of" type="hidden" value="{{BestOf}}"/>
            <input name="warmups" type="hidden" value="{{Warmups}}"/>
        </form>

    {{/Dto.Lobbies}}

    {{^Dto.Lobbies}}
        <p> no lobbies found </p>
    {{/Dto.Lobbies}}

</fieldset>



{{> Footer}}
