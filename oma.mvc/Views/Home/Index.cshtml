@model List<OMA.Models.Dto.LobbyDto>

@{
    ViewData["Title"] = "lobbies";
}

<div class="text-center">
    @if (Model != null)
    {
        foreach (var lobby in Model)
        {
            <a href="/viewlobby?lobby=@lobby.LobbyId">
                <h3> @lobby.LobbyName </h3>
            </a>
        }
    }
</div>
